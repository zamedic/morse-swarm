bot:
 key: <%= @bot_key %>
 username: <%= @data_bag['bot']['name'] %>
<% if @chefNode['morse_swarm']['bot']['proxy']['url'] != nil-%>
proxy:
  url: <%= @chefNode['morse_swarm']['bot']['proxy']['url'] %>
  port: <%= @chefNode['morse_swarm']['bot']['proxy']['port'] %>
<% end -%>
mongo:
 url: <%= @data_bag['mongo']['address'] %>
